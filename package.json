{
    "name": "nearest-neighbour",
    "version": "0.0.0",
    "description": "A simple solution to the nearest neighbour problem written in TypeScript",
    "repository": "https://github.com/samrith-s/nearest-neighbour.git",
    "author": "Samrith Shankar <samrith.shankar@outlook.com>",
    "license": "MIT",
    "private": true,
    "scripts": {
        "clean": "rimraf dist",
        "dev:one": "node scripts/watcher 1",
        "dev:two": "node scripts/watcher 2",
        "build": "yarn clean && tsc -p .",
        "start:one": "yarn build && node dist/src/index 1",
        "start:two": "yarn build && node dist/src/index 2",
        "test": "jest",
        "test:generate": "node scripts/generate-test-cases",
        "lint": "eslint src/**/*"
    },
    "devDependencies": {
        "@commitlint/cli": "^12.0.1",
        "@commitlint/config-conventional": "^12.0.1",
        "@types/jest": "^26.0.20",
        "@types/node": "^14.14.34",
        "@typescript-eslint/eslint-plugin": "^4.17.0",
        "@typescript-eslint/parser": "^4.17.0",
        "chalk": "^4.1.0",
        "chokidar": "^3.5.1",
        "eslint": "^7.22.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-import-resolver-typescript": "^2.4.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.3.1",
        "husky": "^5.1.3",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.5.3",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.3"
    }
}
